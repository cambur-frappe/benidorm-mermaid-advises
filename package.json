{
  "name": "best-business-template",
  "description": "Webcomponent best-business-template following open-wc recommendations",
  "license": "MIT",
  "author": "best-business-template",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "tw": "concurrently \"tailwindcss -i ./src/styles/tailwindlib.css -o ./src/styles/tw.css\" \"twlit --input ./src/styles/tw.css --output ./src/styles/tw.ts\"",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "analyze": "cem analyze --litelement",
    "start": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"web-dev-server\""
  },
  "dependencies": {
    "lit": "^3.1.4"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.10.3",
    "@web/dev-server": "^0.4.6",
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.6.3",
    "twlit": "^0.0.1-alpha.37",
    "typescript": "^5.5.3",
    "vite": "^5.4.4"
  },
  "customElements": "custom-elements.json"
}
